        audio_play_sound(snd_bite,1,false);
        health += obj_spawn_control.ff_health;
        with (ffinstance) {
            instance_destroy();
        }
        sprite_index = spr_fish_lantern;
    }
}

if (health &lt;= 0) {
    dead = true;
    sprite_index = spr_fish_lantern_dead;
    ini_open("Save.ini");
    highscore = score;
    if (highscore &gt; ini_read_real("Save1", "score", 0)) 
        ini_write_real("Save1", "score", highscore);
    ini_close();
}

if (dead == true &amp;&amp; image_index &gt;= image_number - 1) {
